# API Keys Configuration
# Copy this file to .env and fill in your actual API keys

# NewsAPI for real-time news (newsapi.org)
NEWSAPI_KEY=your_newsapi_key_here

# Hugging Face for AI content generation (huggingface.co)
HF_TOKEN=your_huggingface_token_here

# Kit (ConvertKit) for email marketing (kit.com)
KIT_API_KEY=your_kit_api_key_here

# Whop for subscription management (whop.com)
WHOP_API_KEY=your_whop_api_key_here
WHOP_WEBHOOK_SECRET=your_webhook_secret_here

# Optional: Flask settings
FLASK_ENV=production
FLASK_DEBUG=False
